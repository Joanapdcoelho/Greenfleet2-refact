				<munit:attributes value='#[{&#10;    uriParams: {&#10;        ID: "A001"&#10;    }&#10; }]' />
			</munit:set-event>
			<munit-tools:mock-when doc:name="Mock when" doc:id="303a452a-e9de-4cdf-8022-d527f9e72648" processor="http:request" >
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="186eb0af-5ada-45dc-aa63-64e5a91e48b7" attributeName="doc:id" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value='#[output application/json --- readUrl("classpath://account.json")]' />
				</munit-tools:then-return>
